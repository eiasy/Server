<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.adminTool.bo.SystemMallAmountRule" table="system_mall_amount_rule">
		<id name="id" type="int">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="mallId" type="int">
			<column name="mall_id" />
		</property>
		<property name="times" type="int">
			<column name="times" />
		</property>
		<property name="amount" type="int">
			<column name="amount" />
		</property>
	</class>
</hibernate-mapping>
		