<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
		<class name="com.adminTool.bo.SystemReceivePower" table="system_recive_power">
		<composite-id>
		
			<key-property name="id" type="java.lang.Integer">
				<column name="id" not-null="true">
				</column>				
			</key-property>
			<key-property name="type" type="java.lang.Integer">
           		<column name="type" not-null="true">
            	</column>
        	</key-property>
        	<key-property name="remark" type="java.lang.String">
            	<column name="remark" not-null="true">
            	</column>
        	</key-property>
    		<key-property name="startTime" type="java.lang.String">
            	<column name="start_time" not-null="true">
            	</column>
        	</key-property>
        	<key-property name="endTime" type="java.lang.String">
            	<column name="end_time" not-null="true">
            	</column>
        	</key-property>
    	</composite-id>
    </class>
 </hibernate-mapping>