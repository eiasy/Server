<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.adminTool.bo.SystemActivityScratchReward" table="system_activity_scratch_reward">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="activityId" type="java.lang.Integer">
            <column name="activity_id" not-null="true">
            </column>
        </property>
        <property name="rewards" type="java.lang.String">
        	<column name="rewards">
        	</column>
        </property>
        <property name="lowerNum" type="java.lang.Integer">
        	<column name="lower_num">
        	</column>
        </property>
        <property name="upperNum" type="java.lang.Integer">
        	<column name="upper_num">
        	</column>
        </property>
        <property name="isAmend" type="java.lang.Integer">
        	<column name="is_amend">
        	</column>
        </property>
	</class>
</hibernate-mapping>