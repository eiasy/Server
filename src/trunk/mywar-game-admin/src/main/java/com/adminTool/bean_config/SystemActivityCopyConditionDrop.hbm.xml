<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.adminTool.bo.SystemActivityCopyConditionDrop" table="system_activity_copy_condition_drop">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="activityId" type="java.lang.Integer">
            <column name="activity_id" not-null="true">
            </column>
        </property>
        <property name="conditions" type="java.lang.Integer">
        	<column name="conditions">
        	</column>
        </property>
        <property name="rewards" type="java.lang.String">
        	<column name="rewards">
        	</column>
        </property>
	</class>
</hibernate-mapping>