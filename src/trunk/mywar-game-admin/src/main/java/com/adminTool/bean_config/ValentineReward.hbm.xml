<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.adminTool.bo.ValentineReward" table="system_valentine_reward">
        <id name="id" type="java.lang.Integer">
			<column name="id" />
		<generator class="identity" />
		</id>
        <property name="match_num" type="java.lang.Integer">
            <column name="match_num" not-null="true">
            </column>
        </property>	
          <property name="rewards" type="java.lang.String">
            <column name="rewards" not-null="true">
            </column>
        </property>		
</class>
</hibernate-mapping>