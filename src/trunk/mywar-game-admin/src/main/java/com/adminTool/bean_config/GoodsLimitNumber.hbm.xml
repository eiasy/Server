<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.adminTool.bo.GoodsLimitNumber" table="mail_distribute_goods_number">
		<id name="id" type="java.lang.Integer">
			<column name="id"></column>
			<generator class="increment"></generator>
		</id>
		<property name="toolId" type="java.lang.Integer">
			<column  name="tool_id" length="11"></column>
		</property>
		<property name="toolType" type="java.lang.Integer">
			<column name="tool_type" length="11"></column>
		</property>
		<property name="toolName" type="java.lang.String">
			<column name="tool_name" length="50"></column>
		</property>
		<property name="limitNumber" type="java.lang.Integer">
			<column name="limit_number" length="11"></column>
		</property>
	</class>

</hibernate-mapping>