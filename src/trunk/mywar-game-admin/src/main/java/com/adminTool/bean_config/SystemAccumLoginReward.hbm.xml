<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.adminTool.bo.SystemAccumLoginReward" table="system_mid_autumn_reward">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
		<generator class="identity" />
		</id>
		<property name="activityId" type="java.lang.Integer">
		    <column name="activity_id">
		    </column>
		</property>
		<property name="day" type="java.lang.Integer">
            <column name="day" not-null="true">
            </column>
        </property>
        <property name="tools" type="java.lang.String">
        	<column name="tools">
        	</column>
        </property>
	</class>
</hibernate-mapping>